<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/roundedDashView4"
        android:layout_width="0dp"
        android:layout_height="0.8dp"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="60dp"
        android:src="@drawable/bg_border_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/roundedDashView5"
        android:layout_width="0.8dp"
        android:layout_height="0dp"
        android:layout_marginVertical="8dp"
        android:layout_marginEnd="52dp"
        android:src="@drawable/bg_border_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:padding="9dp"
        android:id="@+id/liquor_img"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/img_bottle_vodka"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/liquor_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/binggraesamanco"
        android:text="보드카"
        android:textColor="@color/lim_text_black"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/liquor_img"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.529" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:textOn=""
        android:textOff=""
        android:text=""
        android:background="@drawable/bg_star_select_toggle"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/roundedDashView5"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>