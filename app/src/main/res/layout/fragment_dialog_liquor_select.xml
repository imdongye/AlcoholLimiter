<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LiquorSelectDialogFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@color/lim_back"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/roundedDashView4"
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="?attr/actionBarSize"
            android:src="@drawable/bg_border_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="@color/lim_gray" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scrollbars="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/roundedDashView4">
            <ListView
                android:id="@+id/liquor_list_view"

                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </ListView>
        </androidx.core.widget.NestedScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>