<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/lim_back_d">
    <item>
        <shape
            android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/lim_back_dd"/>
            <solid
                android:color="@color/lim_back_w"/>
            <size
                android:width="50dp"
                android:height="50dp"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape
            android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/lim_back_dd"/>
            <solid
                android:color="@color/lim_back_w"/>
            <size
                android:width="50dp"
                android:height="50dp"/>
        </shape>
    </item>
</ripple>